    //我的方法：除到最后，只要中间不进入if，就是质数
    for(var i = 2; i< num; i++){
      if(num % i === 0){
        alert(num + "不是质数");
        break;//一旦出现一个就不用检查后面了
      }else if(i === num){
        alert(num + "是质数");
      }
    }
    
    //老师的方法：逻辑是一样，但是设置一个flag逻辑更清晰
    var flag = true; //先假设是质数

    for(var i = 2; i< num; i++){
      if(num % i === 0){
        flag = false;
      }
    }

    if(true){
      alert(num + "是质数");
    }
